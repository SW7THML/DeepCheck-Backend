<!-- Modal -->
<div class="modal fade" id="popupPhoto" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title" id="myModalLabel"><%= @course.name %></h4>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
			</div>
			
			<div class="modal-body">
        <form class="form-horizontal">
					<%= time_tag Date.today, prefix: :date%>
					<div class="text-center" id="thumbnail">
					</div>
				</form>
			</div>	

			<div class="modal-footer">

			</div>
		</div>
	</div>
</div>
